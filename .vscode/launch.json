{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "https://localhost:8080",
      "webRoot": "${workspaceFolder}",
      // "userDataDir": false,
      "skipFiles": [
        "<node_internals>/**",
        "**/logClass.ts",
        "**/logClass.js",
        "**/logClass.mjs",
      ],
    },
    {
      "name": "Launch Firefox",
      "request": "launch",
      "type": "firefox",
      "url": "https://localhost:8080",
      "webRoot": "${workspaceFolder}",
      "skipFiles": [
        "<node_internals>/**",
        "**/logClass.ts",
        "**/logClass.js",
        "**/logClass.mjs",
      ],
    },
    {
      "name": "Attach to Chrome",
      "request": "attach",
      "type": "chrome",
      "port": 9222,
      "url": "https://localhost:8080",
      "webRoot": "${workspaceFolder}",
      "skipFiles": [
        "<node_internals>/**",
        "**/logClass.ts",
        "**/logClass.js",
        "**/logClass.mjs",
      ],
    },
  ]
}
